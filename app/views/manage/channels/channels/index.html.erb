<%= link_to t('.add'), new_manage_channels_channel_path  %>
<% if collection.any? %>
  <ul class='item_list'>
    <% collection.each do | channel | %>
      <li>
        <%= channel %>
      </li>
    <% end %>
  </ul>
  <%= paginate collection %>
<% else %>
  <h2 class='empty'><%= t('.empty_list') %></h2>
<% end %>
