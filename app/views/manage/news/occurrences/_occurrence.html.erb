<%= content_for :additional do %>
  <%= content_tag :ul do %>
    <% occurrence.occurrence_properties.each do | property | %>
      <%= content_tag :li do %>
        <%= l(property.since) %>
        <%= l(property.until) %>
        <%= property.location %>
      <% end %>
    <% end %>
  <% end %>
<% end if occurrence.occurrence_properties.exists? %>

<%= render :partial => 'entry', :object => occurrence %>
