<%= semantic_remote_form_for @subtask, :url => issue_subtasks_path(parent) do |form| %>
  <%= form.error_messages %>
  <%= form.inputs do %>
    <%= form.input :executor %>
    <%= form.input :description %>
  <% end %>
  <%= form.buttons do %>
    <%= form.commit_button t('add') %>
  <% end %>
<% end %>
